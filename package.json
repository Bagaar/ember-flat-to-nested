{
  "name": "ember-flat-to-nested",
  "version": "1.0.0",
  "description": "Transforms a flat colocated component structure to a nested colocated component structure.",
  "repository": "https://github.com/bertdeblock/ember-flat-to-nested",
  "license": "MIT",
  "author": "Bert De Block",
  "main": "index.js",
  "bin": "index.js",
  "scripts": {
    "format": "prettier **/*.js --write",
    "lint": "eslint .",
    "test": "ava"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ]
  },
  "ava": {
    "files": [
      "test/test.js"
    ],
    "ignoredByWatcher": [
      "test/output"
    ]
  },
  "dependencies": {
    "@kwsites/file-exists": "^1.1.1",
    "recursive-readdir": "^2.2.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "ava": "^3.14.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "recursive-copy": "^2.0.11"
  }
}
